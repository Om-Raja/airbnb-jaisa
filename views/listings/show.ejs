<% layout("layouts/boilerplate") %>
<head>
  <title><%=list.title%></title>
</head>
<body>
  <h2 class="title-show-route"><%=list.title%></h2>
  <div class="row">
    <div class="col-8 offset-2">
        

  <div class="card">
    <img
      src="<%= list.image %>"
      class="card-img-top show-route-img"
      alt="place view"
    />
    <div class="card-body">
      <h5 class="card-title"><b><%=list.title%></b></h5>
      <p class="card-text">
        <%=list.description%><br />
        <%=list.price%><br />
        <%=list.location%><br />
        <%=list.country%><br />
      </p>
      <div class="btns">

          <a href="/listings/<%=list._id%>/edit" class="btn btn-secondary">Edit</a>
          <form method="POST" action="/listings/<%=list._id%>?_method=DELETE">
              <button class="btn btn-danger">Delete</button>
            </form>
        </div>
    </div>
  </div>
</div>
</div>
</body>


